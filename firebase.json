{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "redirects": [
    {
      "source": "/foo",
      "destination": "/bar",
      "type": 301
    }
  ]
}
